<div class="property">
  <h1>Add property</h1>
<div class="container">
  <form action="/project/add" method="POST" enctype="multipart/form-data">
      <div class="form-group">
          <input type="text" #name name="name" class="form-control" placeholder="Project Name">
      </div>
      <div class="form-group">
          <select name="type" #type  class="form-control">
              <option value="0">--Select--</option>
              {{#each Types}}
              <option value="{{this.id}}">{{this.Name}}</option>
              {{/each}}
          </select>
      </div>
      <div class="form-group">
         <input type="file" class="form-control" #image name="image" id="image">
      </div>
      <div class="form-group">
          <select name="location" #type class="form-control">
              <option value="0">--Select--</option>
              {{#each Locations}}
              <option value="{{this.id}}">{{this.Name}}</option>
              {{/each}}
          </select>
      </div>
      <div class="form-group text-center">
          <button  class="btn btn-primary" type="submit">Add project</button>
      </div>
  </form>
</div>
</div>